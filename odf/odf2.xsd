			<xs:element name="Description" type="presenterDescriptionType" minOccurs="1" maxOccurs="1" />
			<xs:element name="Detail" type="presenterDetailType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Code" type="xs:string" use="optional" />
		<xs:attribute name="MedalRank" type="xs:int"  use="optional" />
		<xs:attribute name="Function" type="xs:string"  use="required" />
		<xs:attribute name="Order" type="xs:int"  use="required" />
	</xs:complexType>

	<xs:complexType name="presenterDescriptionType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="GivenName" use="optional" type="xs:string" />
				<xs:attribute name="FamilyName" use="required" type="xs:string" />
				<xs:attribute name="Gender" use="optional" type="genderCodeType" />
