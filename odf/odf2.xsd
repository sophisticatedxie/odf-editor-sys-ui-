	<xs:element name="OdfBody" type="bodyType">
		<xs:annotation>
			<xs:documentation>ODF2 Message</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="bodyType">
		<xs:annotation>
			<xs:documentation>Message Header</xs:documentation>
		</xs:annotation>
		
		<xs:sequence>
			<xs:element name="Competition" type="competitionType" minOccurs="0" />
			<xs:element name="Note" type="messageType" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="CompetitionCode" type="xs:string" use="required"/>
		<xs:attribute name="DocumentCode" type="xs:string" use="required"/>
		<xs:attribute name="DocumentSubcode" type="xs:string" use="optional"/>
		<xs:attribute name="DocumentType" type="documentTypeType" use="required"/>
		<xs:attribute name="DocumentSubtype" type="xs:string" use="optional" default=""/>
		<xs:attribute name="Version" type="xs:int" use="required" />
		<xs:attribute name="ResultStatus" type="resultStatusType" use="optional"/>
		<xs:attribute name="Language" type="code3Type" use="optional"/>
		<xs:attribute name="FeedFlag" type="feedFlagType" use="required" />
		<xs:attribute name="Date" type="odfDateType" use="required" />
		<xs:attribute name="Time" type="odfTimeType" use="required" />
		<xs:attribute name="LogicalDate" type="odfDateType" use="required" />
		<xs:attribute name="Source" type="xs:string" use="optional" />
		<xs:attribute name="CommandParam1" type="xs:string" use="optional"/>
		<xs:attribute name="CommandParam2" type="xs:string" use="optional"/>
		<xs:attribute name="CommandParam3" type="xs:string" use="optional"/>
		<xs:attribute name="CommandParam4" type="xs:string" use="optional"/>
		<xs:attribute name="CommandParam5" type="xs:string" use="optional"/>
	</xs:complexType>


<!--
	<xs:include schemaLocation="odf2-values.xsd" />
-->

	<xs:complexType name="competitionType">
		<xs:annotation>
			<xs:documentation>Competition element</xs:documentation>
		</xs:annotation>

		<xs:choice>

			<xs:sequence>

				<xs:element name="ExtendedInfos" type="extendedInfosType" minOccurs="0" maxOccurs="1" />
				<xs:element name="Session" type="SessionType" minOccurs="0" maxOccurs="unbounded" />
				<xs:element name="Clock" type="clockType" minOccurs="0" maxOccurs="1" />
				<xs:element name="StatsItems" type="resultStatsType" minOccurs="0" maxOccurs="1" />
				<xs:element name="Officials" type="officialsType" minOccurs="0" maxOccurs="1" />
				<xs:element name="Periods" type="periodsType" minOccurs="0" maxOccurs="1" />
				<xs:element name="Actions" type="unitActionsType" minOccurs="0" maxOccurs="1"/>
				<xs:element name="ImageData" type="pictureType" minOccurs="0"  maxOccurs="1" />

				<xs:element name="Categories" type="ICMSCategoryType" minOccurs="0" maxOccurs="1" />

				<xs:element name="RightsInfo" type="RightsInfoType" minOccurs="0" maxOccurs="1" />

				<xs:element name="ContentInfo" type="ContentInfoType" minOccurs="0" maxOccurs="1" />

				<xs:choice>
					<xs:element name="Result" type="resultType" minOccurs="1" maxOccurs="unbounded" />

					<xs:element name="Unit" type="scheduleUnitType" minOccurs="1" maxOccurs="unbounded" />
					<xs:element name="PDFData" type="messageType" minOccurs="1"	maxOccurs="1" />
					<xs:element name="Bracket" type="bracketType" minOccurs="1" maxOccurs="unbounded" />

					<xs:element name="Stats" type="messageStatsType" minOccurs="1" maxOccurs="1" />
					<xs:element name="Medal" type="medalType" minOccurs="1" maxOccurs="unbounded" />
					<xs:element name="Discipline" type="medallistsDisciplineType"	minOccurs="1" maxOccurs="unbounded" />


					<xs:element name="Communication" type="officialCommunicationType" minOccurs="0" maxOccurs="1" />


					<xs:element name="Participant" type="participantType" maxOccurs="unbounded" />
					<xs:element name="Team" type="teamType" maxOccurs="unbounded" />
					<xs:element name="Horse" type="horseType" maxOccurs="unbounded" />
					<xs:element name="Config" type="confType" minOccurs="0"	maxOccurs="1" />
					<xs:element name="Configs" type="configsType" minOccurs="0"	maxOccurs="1" />
					<xs:element name="Competitor" type="competitorGPSDataType" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="Weather" type="weatherType" minOccurs="0"	maxOccurs="1" />
					<xs:element name="Record" type="brokenRecordType" minOccurs="0" maxOccurs="unbounded" />

					<xs:element name="Image" type="imageType" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="PhotoFinish" type="photoFinishType" minOccurs="0" maxOccurs="1" />

					<xs:element name="DirectLink" type="notificationType" minOccurs="0" maxOccurs="1" />
					<xs:element name="Picture" type="pictureType" minOccurs="0" maxOccurs="1" />

					<xs:element name="MedalStandings" type="medalStandingsType" minOccurs="0" maxOccurs="1" />
					<xs:element name="Document" type="ICMSDocumentType" minOccurs="0" maxOccurs="1" />
					<xs:element name="ParticipantBiography" type="participantBIOType" minOccurs="0" maxOccurs="1" />
					<xs:element name="TeamBiography" type="teamBIOType" minOccurs="0" maxOccurs="1" />
					<xs:element name="HorseBiography" type="horseBIOType" minOccurs="0" maxOccurs="1" />
					<xs:element name="Organisation" type="NOCBIOType" minOccurs="0" maxOccurs="1" />
					<xs:element name="CodeSet" type="CodeSetType" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="Place" type="WeatherPlaceType" minOccurs="0" maxOccurs="1" />

					<xs:element name="Venue" type="WeatherConditionsVenueType" minOccurs="0" maxOccurs="1" />

					<xs:element name="Rankings" type="rankingsType" minOccurs="0" maxOccurs="unbounded" />

					<xs:element name="Presentation" type="presentationType" minOccurs="0" maxOccurs="unbounded" />


					<xs:element name="Ceremony" type="flagBearerType" minOccurs="0" maxOccurs="unbounded" />

				</xs:choice>

			</xs:sequence>

		</xs:choice>

		<xs:attribute name="Gen" type="xs:string" use="optional" />
		<xs:attribute name="Sport" type="xs:string" use="optional" />
		<xs:attribute name="Codes" type="xs:string" use="optional" />

	</xs:complexType>


	<xs:complexType name="extendedInfosType">
		<xs:sequence>
			<xs:element name="DateTime" type="DateTimeType"	minOccurs="0" maxOccurs="1" />
			<xs:element name="UnitDateTime" type="unitDateTimeType"	minOccurs="0" maxOccurs="1" />
			<xs:element name="ExtendedInfo" type="extendedDataType"	minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Progress" type="progressDataType"	minOccurs="0" maxOccurs="1" />
			<xs:element name="SportDescription" type="sportDescriptionDataType"	minOccurs="0" maxOccurs="1" />
			<xs:element name="VenueDescription" type="venueDescriptionDataType" minOccurs="0" maxOccurs="1" />
			<xs:element name="PreviousResults" type="previousResultsDataType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="ListComp" type="ListCompDataType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="SessionType">
		<xs:sequence>
			<xs:element name="SessionName" type="SessionNameType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="SessionCode" type="xs:string" use="required" />
		<xs:attribute name="StartDate" type="xs:dateTime" use="required" />
		<xs:attribute name="EndDate" type="xs:dateTime" use="required" />
		<xs:attribute name="Leadin" type="xs:string"  use="optional" />
		<xs:attribute name="Venue" type="xs:string"  use="required" />
		<xs:attribute name="VenueName" type="xs:string"  use="required" />
		<xs:attribute name="ModificationIndicator" type="modificationIndicatorType" use="optional" />
		<xs:attribute name="SessionStatus" type="scheduleStatusType" use="optional" />
		<xs:attribute name="SessionType" type="xs:string" use="optional" />
		<xs:attribute name="Medal" type="xs:string" use="optional" />

	</xs:complexType>

	<xs:complexType name="SessionNameType">
		<xs:attribute name="Language" type="xs:string" use="required" />
		<xs:attribute name="Value" type="xs:string" use="optional" />
	</xs:complexType>


	<xs:complexType name="clockType">
		<xs:attribute name="Period" type="xs:string" default="" use="optional" />
		<xs:attribute name="Time" type="xs:string" use="required" />
		<xs:attribute name="Running" type="clockRunningType" use="required" />
	</xs:complexType>

	<xs:complexType name="extendedDataType">
		<xs:sequence>
			<xs:element name="Extension" type="extensionType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Competitor" type="unitInfoCompetitorType"	minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string" use="required" />
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string" use="optional" />
	</xs:complexType>

	<xs:complexType name="progressDataType">
		<xs:sequence>
			<xs:element name="Extension" type="extensionType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="LastUnit" type="xs:string" use="optional" />
		<xs:attribute name="UnitsTotal" type="xs:int" use="optional" />
		<xs:attribute name="UnitsComplete" type="xs:int"  use="optional" />
	</xs:complexType>


	<xs:complexType name="extendedResultsDataType">
		<xs:sequence>
			<xs:element name="Extension" type="extensionType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string" use="required" />
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string"  use="optional" />
		<xs:attribute name="Value2" type="xs:string"  use="optional" />
		<xs:attribute name="ValueType" type="xs:string" default="" use="optional" />
		<xs:attribute name="IRM" type="xs:string" default="" use="optional" />
		<xs:attribute name="Rank" type="xs:string" default="" use="optional" />
		<xs:attribute name="RankEqual" type="rankEqualType" use="optional" />
		<xs:attribute name="SortOrder" type="xs:integer" use="optional" />
		<xs:attribute name="Diff" type="xs:string" default="" use="optional" />
		<xs:attribute name="Speed" type="xs:string" default="" use="optional" />
		<xs:attribute name="Move" type="xs:string" default="" use="optional" />
		<xs:attribute name="Pty" type="xs:string"  use="optional" />
		<xs:attribute name="Discard" type="xs:string"  use="optional" />
		<xs:attribute name="Arrive" type="xs:string"  use="optional" />
		<xs:attribute name="Unchecked" type="UncheckedType" use="optional" />
	</xs:complexType>

	<xs:complexType name="extensionType">
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string"  use="optional" />

	</xs:complexType>

	<xs:complexType name="unitDateTimeType">
		<xs:attribute name="StartDate" type="xs:dateTime" use="optional" />
		<xs:attribute name="EndDate" type="xs:dateTime" use="optional" />
		<xs:attribute name="Duration" type="xs:string"  use="optional" />
	</xs:complexType>

	<xs:complexType name="DateTimeType">
		<xs:attribute name="Update" type="odfDateType" use="required" />
		<xs:attribute name="PreviousUpdate" type="odfDateType" use="optional" />
		<xs:attribute name="NextUpdate" type="odfDateType" use="optional" />
	</xs:complexType>

	<xs:complexType name="sportDescriptionDataType">
		<xs:attribute name="DisciplineName" type="xs:string" use="required" />
		<xs:attribute name="EventName" type="xs:string" default="" use="optional" />
		<xs:attribute name="SubEventName" type="xs:string" default="" use="optional" />
		<xs:attribute name="Gender" type="disciplinegenderCodeType" use="optional" />
		<xs:attribute name="UnitNum" type="xs:string" default="" use="optional" />
	</xs:complexType>

	<xs:complexType name="venueDescriptionDataType">
		<xs:attribute name="Venue" type="code3Type" use="required" />
		<xs:attribute name="VenueName" type="xs:string" use="required" />
		<xs:attribute name="Location" type="code3Type" use="optional" />
		<xs:attribute name="LocationName" type="xs:string" default="" use="optional" />
		<xs:attribute name="Attendance" type="xs:int" use="optional" />
	</xs:complexType>

	<xs:complexType name="previousResultsDataType">
		<xs:sequence>
			<xs:element name="PreviousResult" type="PreviousResultDataType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Win" type="xs:string" use="optional" />
		<xs:attribute name="Loss" type="xs:string" use="optional" />
		<xs:attribute name="Tie" type="xs:string" default="" use="optional" />
	</xs:complexType>


	<xs:complexType name="ListCompDataType">
		<xs:attribute name="Date" type="odfDateType" use="required" />
		<xs:attribute name="Place" type="xs:string" default="" use="optional" />
		<xs:attribute name="Country" type="xs:string" default="" use="optional" />
		<xs:attribute name="Pos" type="xs:string" use="required" />
		<xs:attribute name="AddInfo" type="xs:string" default="" use="optional" />
		<xs:attribute name="Status" type="xs:string" default="" use="optional" />
	</xs:complexType>


	<xs:complexType name="PreviousResultDataType">
		<xs:sequence>
			<xs:element name="ExtendedResults" type="extendedResultsType" minOccurs="0" maxOccurs="1" />
			<xs:element name="Partial" type="previousPartialResultsDataType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Unit" type="xs:string" use="optional" />
		<xs:attribute name="Order" type="xs:int" use="required" />
		<xs:attribute name="Opponent" type="xs:string" use="optional" />
		<xs:attribute name="WLT" type="xs:string" default="" use="optional" />
		<xs:attribute name="Result" type="xs:string" default="" use="optional" />
		<xs:attribute name="OppResult" type="xs:string" default="" use="optional" />
		<xs:attribute name="ResultType" type="xs:string" default="" use="optional" />
		<xs:attribute name="IRM" type="xs:string" default="" use="optional" />
	</xs:complexType>

	<xs:complexType name="previousPartialResultsDataType">
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Score" type="xs:string" use="optional" />
		<xs:attribute name="OppScore" type="xs:string" use="optional" />
	</xs:complexType>

	<xs:complexType name="unitInfoCompetitorType">
		<xs:sequence>
			<xs:element name="Composition" type="unitInfoCompositionType" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="Organisation" type="xs:string" default="" use="optional" />
		<xs:attribute name="Order" type="xs:int" use="optional" />
	</xs:complexType>

	<xs:complexType name="unitInfoCompositionType">
		<xs:sequence>
			<xs:element name="Athlete" type="unitInfoAthleteType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="unitInfoAthleteType">
		<xs:attribute name="FamilyName" type="xs:string" use="required" />
		<xs:attribute name="GivenName" type="xs:string" default="" use="optional" />
	</xs:complexType>


	<xs:complexType name="officialsType">
		<xs:sequence>
			<xs:element name="Official" type="officialResultsType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="officialResultsType">
		<xs:sequence>
			<xs:element name="Description" type="officialResultsDescriptionType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ExtOfficial" type="extOfficialType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Function" type="xs:string" use="optional" />
		<xs:attribute name="Order" type="xs:string"  use="optional" />
		<xs:attribute name="Bib" type="xs:string"  use="optional" />
	</xs:complexType>

	<xs:complexType name="officialResultsDescriptionType">
		<xs:sequence>
			<xs:element name="ExtendedDescription" type="extendedDescriptionType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="GivenName" type="xs:string" default="" use="optional" />
		<xs:attribute name="FamilyName" type="xs:string" use="required" />
		<xs:attribute name="Gender" type="genderCodeType" use="required" />
		<xs:attribute name="Organisation" type="xs:string" use="required" />
		<xs:attribute name="IFId" type="xs:string" default="" use="optional" />
	</xs:complexType>

	<xs:complexType name="extendedDescriptionType">
		<xs:attribute name="Type" type="xs:string" use="required" />
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string"  use="optional" />
	</xs:complexType>

	<xs:complexType name="extOfficialType">
		<xs:attribute name="Type" type="xs:string" use="required" />
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string"  use="optional" />
	</xs:complexType>

	<xs:complexType name="periodsType">
		<xs:sequence>
			<xs:element name="Period" type="periodType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="Home" type="xs:string" default="" use="optional" />
		<xs:attribute name="Away" type="xs:string" default="" use="optional" />
	</xs:complexType>

	<xs:complexType name="periodType">
		<xs:sequence>
			<xs:element name="ExtendedPeriods" type="extendedPeriodsType" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="HomeScore" type="xs:string" default="" use="optional" />
		<xs:attribute name="AwayScore" type="xs:string" default="" use="optional" />
		<xs:attribute name="HomePeriodScore" type="xs:string" default="" use="optional" />
		<xs:attribute name="AwayPeriodScore" type="xs:string" default="" use="optional" />
		<xs:attribute name="Duration" type="xs:string" default="" use="optional" />
	</xs:complexType>

	<xs:complexType name="extendedPeriodsType">
		<xs:sequence>
			<xs:element name="ExtendedPeriod" type="extendedPeriodType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="extendedPeriodType">
		<xs:attribute name="Type" type="xs:string" use="required" />
		<xs:attribute name="Code" type="xs:string" use="required" />
		<xs:attribute name="Pos" type="xs:string"  use="optional" />
		<xs:attribute name="Value" type="xs:string"  use="optional" />
	</xs:complexType>

	<xs:complexType name="resultType">
		<xs:sequence>
			<xs:element name="ExtendedResults" type="extendedResultsType" minOccurs="0" maxOccurs="1" />
			<xs:element name="RecordIndicators" type="recordIndicatorsType" minOccurs="0" maxOccurs="1" />
			<xs:element name="ResultItems" type="resultItemsType" minOccurs="0" />
			<xs:element name="Competitor" type="competitorResultsType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>

